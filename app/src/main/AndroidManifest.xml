<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gamaya">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gamaya">
        <activity android:name=".GambarTarianActivity"></activity>
        <activity android:name=".MusikActivity" />
        <activity android:name=".TarianActivity" />
        <activity android:name=".PakaianActivity" />
        <activity android:name=".RumahActivity" />
        <activity android:name=".MenuMateriActivity" />
        <activity
            android:name=".EvalActivity"
            android:configChanges="orientation|screenSize|keyboardHidden|smallestScreenSize|screenLayout" />
        <activity android:name=".TebakGambarActivity" />
        <activity android:name=".ViewPagerActivity" />
        <activity android:name=".QuizResultActivity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".PetunjukActivity" />
        <activity android:name=".ProfileActivity" />
        <activity
            android:name=".HomeActivity"
            android:launchMode="singleInstance" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>